# BBY jump box
Host bby-admin
    # Hostname pipelines-burberry.wiredcraft.cn
    Hostname 52.83.164.162
    Port 22
    User wcladmin

# Tool server
Host bby-registry
    Hostname registry
    User wcladmin
    ProxyCommand ssh -W %h:%p -q bby-admin

